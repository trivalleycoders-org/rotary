{
  "name": "rotary-server",
  "version": "0.1.0",
  "description": "server for rotary project",
  "scripts": {
    "start": "nodemon server/server.js --exec babel-node",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha --compilers js:babel-core/register server/**/*.test.js ",
    "build": "babel . -d dist --presets es2015,stage-2 --ignore spec.js,node_modules",
    "serve": "node dist/server.js",
    "start_babel": "babel-node server/server.js --preset=babel-preset-es2015",

    "test-watch": "nodemon --exec 'npm test'",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://"
  },
  "keywords": [
    "Node.js",
    "Javascript",
    "Express"
  ],
  "author": "Tom Yitav <tomyitav@gmail.com>",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.8.0",
    "dotenv": "5.0.1",
    "expect": "22.4.3",
    "express": "4.14.0",
    "jest": "22.4.3",
    "lodash": "^4.15.0",
    "mocha": "5.1.0",
    "mongoose": "^5.0.14",
    "morgan": "^1.8.2",
    "supertest": "3.0.0",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-inline-import": "^2.0.1",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "4.19.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-react": "7.7.0",
    "nodemon": "^1.9.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
